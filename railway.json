{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "install": {
          "cmds": [
            "pip install -r requirements.txt"
          ]
        }
      }
    }
  },
  "deploy": {
    "healthcheckPath": "/health",
    "startCommand": "gunicorn -k gthread -w 1 -t 120 backend.app:app --bind 0.0.0.0:$PORT"
  }
}
